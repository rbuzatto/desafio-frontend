{
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "prettier/prettier": "error",
    "comma-dangle": 0,
    "react/jsx-uses-vars": 1,
    "react/display-name": 1,
    "no-unused-vars": "warn",
    "no-console": 1,
    "no-unexpected-multiline": "warn",
    "react/react-in-jsx-scope": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
